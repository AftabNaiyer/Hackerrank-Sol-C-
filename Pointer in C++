//--------------------------------//
SOLUTION
//-------------------------------//

#include <stdio.h>
void update(int *a,int *b) {
    // Complete this function  
    int sum = *a+*b;
    int mult;
    if(*a>*b)
    {
         mult = *a-*b;
    }  
    else
    {
        mult = *b-*a;
    }
     *a = sum;
   *b = mult;
}
int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);
    return 0;
}



//--------------------------------//
EXPLANATION
//--------------------------------//

a’ = 4 + 5 = 9
b’ = | 4 – 5 | = 1
